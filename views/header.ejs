
<nav class="navbar navbar-fixed-top" role="navigation">

    <div class="container" id="header">
        <!-- Brand and toggle get grouped for better mobile display -->

            <a class="navbar-brand" href="#"><img src="coverme.svg" class="logo vcenter" alt=""/></a>
            <div class="searchForm" style="border: 1px black solid">
                <form role="search" id="searchForm">
                    <input class="input-large typeahead tt-query" id="searchBox" type="text" placeholder="Search for Covers"  autocomplete="off" spellcheck="false"/><!--class="form-control" -->
                    <a id="searchButton" class="btn btn-default" type="submit"><span>Search</span></a>

                </form>
            </div>





        <!-- Collect the nav links, forms, and other content for toggling -->

        <script>
            $('#searchButton').click(function() {
                location.href = this.href + ('/search?q=') + $('#searchBox').val();
                return false;
            });
            $("#searchBox").keypress(function(event){
                if(event.keyCode == 13){
                    event.preventDefault();
                    $("#searchButton").click();
                }
            });
        </script>
    </div>
    <!-- /.container-fluid -->
</nav>