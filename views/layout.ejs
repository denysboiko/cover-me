    <script>
        var ias = $.ias({
            container:  '#covers',
            item:       '.cover',
            pagination: '#pagination',
            next:       '.next',
            delay: 1000,
            negativeMargin: 100
        });
        ias.on('load', function(event) {
            jQuery.ias().off('scroll');
        })
        ias.on('rendered', function(data, items) {
            jQuery.ias().on('scroll');
        })
        ias.on('noneLeft', function() {
            $(".ias-spinner").hide();
        })

    </script>
