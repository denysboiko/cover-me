    <![endif]-->


    <!--<script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>-->
    <script src="/js/jquery-1.8.3.min.js"></script>
    <script src="/bootstrap/js/bootstrap.min.js"></script>
    <script src="/js/emerge.min.js"></script>
    <script src="jquery-ias.min.js"></script>
    <script>
        var ias = jQuery.ias({
            container:  '#covers',
            item:       '.cover',
            pagination: '#pagination',
            next:       '.next'
        });
        ias.extension(new IASSpinnerExtension());
    </script>

    <link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel='stylesheet' href='/css/style.css'/>

    <link rel="shortcut icon" href="favicon.ico"/>
</head>


<body>
<script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
        (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-55360284-1', 'auto');
    ga('send', 'pageview');

</script>

<% include header.ejs %>

<div class="container" style="background-color: white; float: none; margin: 135px auto 0; max-width: 700px;">
    <ul id="search" class="list-group">
<div class="container" style="background-color: white; float: none; margin: 0 auto; max-width: 700px">
    <ul class="list-group">
        <% for(var i=0; i<searchResults.length; i++) {%>
        <li>
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="panel-more">
                        <img class="emerge" src="<%=searchResults[i]._source.bPicture%>" style="width: 100px">
                    </div>
                    <div class="panel-info">
                        <h4><strong><%=searchResults[i]._source.artist%></strong></h4>
                        <small><%=searchResults[i]._source.album%></small>
                    </div>
                    <div class="panel-rating pull-right">
                        <a href="" class="btn btn-primary" role="button" download="<%=searchResults[i]._source.bPicture%>">Download <span class="glyphicon  glyphicon-download"></span></a>
                    </div>

                </div>
            </div>
        </li>
        <% } %>

    </ul>
</div>

</body>

</html>

<script>
    $(document).ready(function(){
        $('#navbar').css('background-color', 'darkturquoise');
        $('#header').css('display', 'block');
    });
</script>