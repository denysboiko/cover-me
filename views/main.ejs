<!DOCTYPE html>
<html>
<head>
    <title></title>


    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


    <!--<script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>-->
    <script src="/js/jquery-1.8.3.min.js"></script>
    <script src="/bootstrap/js/bootstrap.min.js"></script>
    <script src="/js/emerge.min.js"></script>
    <script src="jquery-ias.min.js"></script>
    <script>
        var ias = jQuery.ias({
            container:  '#covers',
            item:       '.cover',
            pagination: '#pagination',
            next:       '.next'
        });
        ias.extension(new IASSpinnerExtension());
    </script>

    <link href="/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link rel='stylesheet' href='/css/style.css'/>

    <link rel="shortcut icon" href="favicon.ico"/>
</head>


<body>

<% include header.ejs %>

<div class="container" style="background-color: white; float: none; margin: 0 auto; max-width: 700px;">
    <ul class="list-group">
        <% for(var i=0; i<searchResults.length; i++) {%>
        <li>
            <div class="panel panel-default" style="border-radius: 0px; border-color: white;">
                <div class="panel-body">
                    <div class="panel-more1">
                        <img class="emerge" src="<%=searchResults[i]._source.bPicture%>" style="width: 100px">
                    </div>
                    <div class="panel-info">
                        <h2><strong><%=searchResults[i]._source.artist%></strong></h2>
                        <small><%=searchResults[i]._source.album%></small>
                    </div>
                    <div class="panel-rating pull-right">
                        <a href="" class="btn btn-primary" role="button" download="img.jpg">Download <span class="glyphicon  glyphicon-download"></span></a>
                    </div>

                </div>
            </div>
        </li>
        <% } %>

    </ul>
</div>

</body>

</html>